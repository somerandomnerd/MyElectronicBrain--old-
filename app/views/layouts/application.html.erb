<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <%= csrf_meta_tag %>
  <%= render 'layouts/stylesheets' %>
  <%= javascript_include_tag :defaults %>
</head>
<body>
<div id="page">
  <header>
  <%= render 'layouts/header' %>
  </header>
  <%= yield %>
  <footer>
  <%= render 'layouts/footer' %>
  </footer>
  <%= debug(params) if Rails.env.development? %>
</div>
</body>
</html>
